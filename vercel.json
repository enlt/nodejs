{
  "version": 2,
  "builds": [
    {
      "src": "a/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/a(.*)",
      "dest": "/a/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}